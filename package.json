{
  "name": "contact",
  "version": "2.0.0",
  "description" : "KaiOS core app for contacts management",
  "main": "src/_app.js",
  "author": "hank.chiu@kaiostech.com",
  "scripts": {
    "build": "webpack",
    "server": "node start.js",
    "test": "jest --no-cache --verbose --updateSnapshot",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rm build/bundle.js",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:js": "eslint src --quiet",
    "lint:css": "stylelint 'scss/*.scss'"
  },
  "precommit": [
    "lint"
  ],
  "devDependencies": {
    "babel-core": "6.18.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "^22.4.3",
    "babel-loader": "6.4.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.3.13",
    "babel-preset-react-app": "2.0.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "babel-resolver": "1.1.0",
    "browser-env": "2.0.21",
    "coveralls": "2.11.9",
    "css-loader": "0.23.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15.4": "^1.0.5",
    "enzyme-to-json": "^3.3.3",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.8.5",
    "ignore-styles": "5.0.1",
    "jest": "^22.4.3",
    "jest-css-modules": "^1.1.0",
    "node-sass": "4.7.2",
    "nyc": "6.4.0",
    "pre-commit": "1.2.2",
    "react-addons-test-utils": "15.1.0",
    "sass-loader": "3.2.3",
    "sinon": "1.17.6",
    "style-loader": "0.13.1",
    "stylelint": "7.10.1",
    "stylelint-config-standard": "16.0.0",
    "webpack": "2.7.0"
  },
  "dependencies": {
    "base-component": "git+ssh://git@git.kaiostech.com:shared/base_component.git",
    "base-emitter": "git+ssh://git@git.kaiostech.com:shared/base-emitter.git",
    "base-module": "git+ssh://git@git.kaiostech.com:shared/base-module.git",
    "chance": "1.0.9",
    "common-scss": "git+ssh://git@git.kaiostech.com:shared/common-scss.git",
    "enhance-animation": "git+ssh://git@git.kaiostech.com:shared/enhance-animation.git",
    "fatigue-scripts": "git+ssh://git@git.kaiostech.com:shared/fatigue-scripts.git",
    "gaia-icons": "git+ssh://git@git.kaiostech.com:shared/gaia-icons.git",
    "l10n": "git+ssh://git@git.kaiostech.com:shared/l10n.git",
    "locales": "git+ssh://git@git.kaiostech.com:shared/locales.git",
    "mime-mapper": "git+ssh://git@git.kaiostech.com:shared/mime-mapper.git",
    "mobile-operator": "git+ssh://git@git.kaiostech.com:shared/mobile-operator.git",
    "react": "15.3.2",
    "react-dialog": "git+ssh://git@git.kaiostech.com:shared/react-dialog.git#0.0.24",
    "react-dom": "15.3.2",
    "react-option-menu": "git+ssh://git@git.kaiostech.com:shared/react-option-menu.git",
    "react-sim-chooser": "git+ssh://git@git.kaiostech.com:shared/react-sim-chooser.git",
    "react-soft-key": "git+ssh://git@git.kaiostech.com:shared/react-soft-key.git",
    "react-window-manager": "git+ssh://git@git.kaiostech.com:shared/react-window-manager.git",
    "sdcard-manager": "git+ssh://git@git.kaiostech.com:shared/sdcard-manager.git",
    "service": "git+ssh://git@git.kaiostech.com:shared/service.git",
    "sim-settings-helper": "git+ssh://git@git.kaiostech.com:shared/sim-settings-helper.git",
    "simple-navigation-helper": "git+ssh://git@git.kaiostech.com:shared/simple-navigation-helper.git",
    "simslot-manager": "git+ssh://git@git.kaiostech.com:shared/simslot-manager.git",
    "soft-key-store": "git+ssh://git@git.kaiostech.com:shared/soft-key-store.git"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "modulePaths": [
      "<rootDir>/src/",
      "<rootDir>/scss/"
    ],
    "setupFiles": [
      "./test/setup/jestSetup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.yarn-cache/"
    ],
    "transform": {
      "\\.(scss|css)$": "<rootDir>/node_modules/jest-css-modules",
      "^.+\\.jsx?$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|properties)$": "<rootDir>/test/setup/fileTransformer.js"
    },
    "transformIgnorePatterns": []
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2",
      "react"
    ],
    "plugins": [
      "dynamic-import-node"
    ]
  }
}
